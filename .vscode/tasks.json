{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "run go",
            "type": "shell",
            "command": "go run ${relativeFile}",
            "group": "build",
            "problemMatcher": [
                "$go"
            ]
        },
        {
            "label": "run python",
            "type": "shell",
            "command": "python ${relativeFile}",
            "group": "build",
            "problemMatcher": []
        }
    ]
}